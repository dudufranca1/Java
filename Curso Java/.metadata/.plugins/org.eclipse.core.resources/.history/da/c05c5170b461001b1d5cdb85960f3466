package com.estudando.exerciciossb.models;

public class Calculadora {

	private int a, b;

	public Calculadora(int a, int b) {
		super();
		this.a = a;
		this.b = b;
	}

	public Calculadora(int i) {
		// TODO Auto-generated constructor stub
	}

	public int getA() {
		return a;
	}

	public void setA(int a) {
		this.a = a;
	}

	public int getB() {
		return b;
	}

	public void setB(int b) {
		this.b = b;
	}
	
	
	
}
